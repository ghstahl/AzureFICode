<div class="activity">
  <div class="container">
    <div class='col-md-5'>
      <div class="form-group">
        <div class='input-group date' id='activityFromDate'>
          <input type='text' class="form-control" placeholder="From Date" />
          <span class="input-group-addon">
            <span class="glyphicon glyphicon-calendar"></span>
          </span>
        </div>
      </div>
    </div>
    <div class='col-md-5'>
      <div class="form-group">
        <div class='input-group date' id='activityToDate'>
          <input type='text' class="form-control" placeholder="To Date" />
          <span class="input-group-addon">
            <span class="glyphicon glyphicon-calendar"></span>
          </span>
        </div>
      </div>
    </div>
    <div class='col-md-2'>
      <div class="form-group">
        <input type='button' class="form-control" value="Search" id="activitySearch" />
      </div>
    </div>
  </div>

  <table class="table table-bordered">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Resource Id</th>
        <th scope="col">Chaos Operation</th>
        <th scope="col">Chaos Started Time</th>
        <th scope="col">Chaos Completed Time</th>
        <th scope="col">Initial State</th>
        <th scope="col">Final State</th>
        <th scope="col">Status</th>
        <th scope="col">Error</th>
        <th scope="col">Warning</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>
